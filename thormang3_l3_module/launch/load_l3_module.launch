<?xml version="1.0"?>

<launch>
  <!-- start robot model server -->
  <include file="$(find thor_mang_l3_config)/launch/robot_model_server.launch" />

  <rosparam file="$(find thormang3_l3_module)/config/l3_module_config.yaml" command="load" />
  <rosparam file="$(find thormang3_l3_module)/config/plugin_sets.yaml" command="load" />

  <param name="step_controller_plugin" value="zmp_walk_step_controller_plugin" />
  <param name="step_plan_msg_plugin" value="l3_step_plan_msg_plugin" />
  <param name="plugin_set" value="robotis_walk" />
</launch>

